{
	"extends": "standard",
	"plugins": [
		"html"
	],
	"env": {
		"jasmine": true
	},
	"rules": {
		"no-var": "error",
		"indent": "off",
		"quotes": "off",
		"semi": ["error", "always"],
		"space-before-function-paren": "off",
		"keyword-spacing": "off",
		"brace-style": ["error", "stroustrup"],
		"no-tabs": 0
	}
}